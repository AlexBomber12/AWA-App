# Default: SQLite at /data/awa.db
#DATABASE_URL=sqlite+aiosqlite:///data/awa.db
# Live Postgres
DATABASE_URL=postgresql+psycopg://postgres:pass@postgres:5432/postgres

# Optional Postgres settings (override DATABASE_URL automatically)
PG_PASSWORD=
# These values build the DATABASE_URL for Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=pass
POSTGRES_DB=postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

